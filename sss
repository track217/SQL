first_var <- c()
for(i in 3:ncol(train_data))
  {
    train=train_data[,c(1,2,i)]
    results <- boot(data=train, statistic=chi, R=100, formula=Surv(drfs_even_time_years,drfs_1_event_0_censored)~.)
    if (sum(results$t <0.001)>49) 
      {
        first_var<-c(first_var, i)
        print(i)
      }
  }
